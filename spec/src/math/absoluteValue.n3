@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fnon: <https://w3id.org/function/ontology/n3#> .

math:absoluteValue a fno:Function ;
    vs:term_status  "testing" ;
    fnon:tldr "Calculates as object the absolute value of the subject." ;
    dcterms:comment "`true` if and only if `$o` is the absolute value of `$s`." ;
    fno:example ( [ a fno:Test ;
                dcterms:description """
Calculate the absolute value of the value -2.
            """ ;
                rdfs:seeAlso <> ;
                fno:expression """
@prefix : <http://example.org/>.
@prefix math: <http://www.w3.org/2000/10/swap/math#> .

:Let :param -2 .

{
    :Let :param ?param .
    ?param math:absoluteValue ?result .
}
=>
{
    :result :is ?result .
} .
            """ ;
                fno:result """
@prefix : <http://example.org/>.
:result :is 2. 
            """ ] ) ;
    fno:name "absoluteValue" ;
    fno:parameter ( [
        a fno:Parameter ;
        fno:mode "+" ;
        fno:predicate "$s" ;
        fno:type 
            [   rdf:type rdfs:Datatype ;
                owl:unionOf ( xsd:decimal xsd:double xsd:float )
            ] ;
        fnon:position fnon:subject
    ] [
        a fno:Parameter ;
        fno:mode "-" ;
        fno:predicate "$o" ;
        fno:type
            [   rdf:type rdfs:Datatype ;
                owl:unionOf ( xsd:decimal xsd:double xsd:float )
            ] ;
        fnon:position fnon:object
    ] ) .
